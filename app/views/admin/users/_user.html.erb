<tr>
  <td><%= user.name %></td>
  <% if !user.admin? %>    
    <td><%= link_to t('common.show'), [:admin, user], class:'btn' %></td>
    <td><%= link_to t('common.edit'), edit_admin_user_path(user), class:'btn' %></td>
    <td><%= link_to t('common.delete'), [:admin, user], class:'btn btn-danger', method: :delete, data: { confirm: "#{t('common.confirm')  }"} %></td>
  <% end %>
</tr>